    "debug:server": "pnpm --filter \"*backend\" start:debug",
    "debug:logserver": "pnpm --filter \"*backend\" logserver:dev",
    "build:bundler": "pnpm --filter \"*bundler\" build",
    "prisma:generate": "pnpm --filter \"*backend\" prisma:generate",
    "prisma:push": "pnpm --filter \"*backend\" prisma:push",
    "caddy:run": "caddy run --config Caddyfile --watch",
    "build:plugin-bundler": "pnpm --filter \"*plugin-bundler\" --filter \"*editor\" build"
